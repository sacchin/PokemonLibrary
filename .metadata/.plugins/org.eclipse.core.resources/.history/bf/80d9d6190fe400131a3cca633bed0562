package com.gmail.sacchin.pockemonbattletools.entity;

import org.json.JSONObject;

import com.gmail.sacchin.pokemonlibrary.entity.Skill;

public class Waza extends Skill{
	private double rate = 0;
	

	public Waza(JSONObject waza){
		String name = waza.getString("name");
		int ranking = waza.getInt("ranking");
		int type = waza.getInt("typeId");
		rate = waza.getDouble("usageRate");
		this(0, name, name, type, 0, 0, 0, 0);

	}
			
			
			
	public Waza(int no, String ‚Šname, String ename, int type, int power,
			int accuracy, int category, int pp) {
		super(no, ‚Šname, ename, type, power, accuracy, category, pp);
	}
	
	

}
